[Unit]
Description=Cmux VNC WebSocket proxy
After=cmux-x11vnc.service
Requires=cmux-x11vnc.service

[Service]
Type=simple
ExecStartPre=/bin/mkdir -p /var/log/cmux
ExecStart=/usr/local/bin/vnc-ws-proxy --listen 0.0.0.0:39380 --target 127.0.0.1:5901
Restart=always
RestartSec=3
StandardOutput=append:/var/log/cmux/vnc-ws-proxy.log
StandardError=append:/var/log/cmux/vnc-ws-proxy.log

[Install]
WantedBy=cmux.target
