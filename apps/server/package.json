{
  "name": "@cmux/server",
  "version": "1.0.0",
  "description": "cmux server",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "type": "module",
  "scripts": {
    "start": "vite-node src/dev-server.ts",
    "dev3": "DENO_COMPAT=1 deno run -A --watch-hmr src/dev-server.ts",
    "dev2": "vite-node src/dev-server.ts",
    "dev": "bun run --hot src/dev-server.ts",
    "dev0": "DENO_COMPAT=1 deno run -A src/dev-server.ts",
    "dev-vitenode": "vite-node --watch src/dev-server.ts",
    "start-deno": "deno run -A src/dev-server.ts",
    "dev-deno": "deno run -A --watch-hmr src/dev-server.ts",
    "start-bun": "bun run src/dev-server.ts",
    "dev-bun": "bun run --hot src/dev-server.ts",
    "daytona": "bun x vite-node scripts/hello-daytona.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@cmux/convex": "workspace:*",
    "@cmux/shared": "workspace:*",
    "@daytonaio/sdk": "^0.22.1",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/headless": "^5.5.0",
    "ansi-escape-sequences": "^6.2.4",
    "archiver": "^7.0.1",
    "chokidar": "^4.0.3",
    "convex": "1.26.0-alpha.6",
    "dockerode": "^4.0.2",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "fuzzysort": "^3.1.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "httpxy": "^0.1.7",
    "ignore": "^7.0.5",
    "minimatch": "^10.0.3",
    "morphcloud": "^0.0.14",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "tar": "^7.4.3",
    "vite-node": "^3.2.4",
    "ws": "^8.18.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/dockerode": "^3.3.31",
    "@types/express": "^4.17.23",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^24.1.0",
    "@types/ws": "^8.18.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^2.1.8"
  }
}