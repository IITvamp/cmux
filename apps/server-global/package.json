{
  "name": "@cmux/server-global",
  "type": "module",
  "private": true,
  "main": "dist/serve.js",
  "scripts": {
    "dev": "bun run --hot  --env-file ../../.env src/index.ts",
    "build": "esbuild src/serve.ts --bundle --platform=node --outdir=dist --format=esm --packages=external",
    "start": "node dist/serve.js",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "esbuild": "^0.25.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.1",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@stackframe/js": "^2.8.28",
    "@t3-oss/env-core": "^0.13.8",
    "hono": "^4.9.1",
    "zod": "^4.0.17"
  }
}