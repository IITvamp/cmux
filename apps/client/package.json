  "description": "cmux client application",
  "author": "cmux",
  "type": "module",
  "private": true,
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "NEXT_PUBLIC_CONVEX_URL=http://localhost:9777 electron-vite dev -c electron.vite.config.js",
    "build": "tsc -b && vite build -c vite.config.ts",
    "build:electron": "electron-vite build -c electron.vite.config.js && electron-builder",
    "build:mac": "electron-vite build -c electron.vite.config.js && electron-builder --mac",
    "build:mac:workaround": "./build-mac-workaround.sh",
    "dist": "electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:electron": "electron-vite preview -c electron.vite.config.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "generate-routes": "tsr generate"
  },
