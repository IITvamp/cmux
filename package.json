{
  "name": "coderouter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "scripts"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@coderouter/client",
    "build": "npm run build --workspace=@coderouter/client",
    "lint": "npm run lint --workspace=@coderouter/client",
    "preview": "npm run preview --workspace=@coderouter/client",
    "server": "npm run dev --workspace=@coderouter/server",
    "typecheck": "run-p typecheck:*",
    "typecheck:client": "npm run typecheck --workspace=@coderouter/client",
    "typecheck:server": "cd apps/server && tsc --noEmit",
    "typecheck:shared": "cd packages/shared && tsc --noEmit",
    "typecheck:convex": "cd packages/convex && tsc --noEmit",
    "typecheck:cmux": "cd packages/cmux && tsc --noEmit",
    "generate-routes": "npm run generate-routes --workspace=@coderouter/client"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "postinstall": "patch-package",
  "packageManager": "pnpm@10.13.1"
}