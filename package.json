{
  "name": "cmux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "bash scripts/clean.sh",
    "clean:node_modules": "bash scripts/clean.sh",
    "typecheck": "run-p typecheck:*",
    "typecheck:client": "cd apps/client && bun run typecheck",
    "typecheck:server": "cd apps/server && bun run typecheck",
    "typecheck:shared": "cd packages/shared && bun run typecheck",
    "typecheck:worker": "cd apps/worker && bun run typecheck",
    "typecheck:vscode-extension": "cd packages/vscode-extension && bun run typecheck",
    "typecheck:convex": "cd packages/convex && bun run typecheck",
    "typecheck:cmux": "cd packages/cmux && bun run typecheck",
    "typecheck:preview-service": "cd apps/preview-service && bun run typecheck",
    "typecheck:landing": "cd apps/landing && pnpm run typecheck",
    "landing:dev": "cd apps/landing && pnpm run dev",
    "landing:build": "cd apps/landing && pnpm run build",
    "landing:start": "cd apps/landing && pnpm run start",
    "generate-routes": "cd packages/client && bun run generate-routes"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "5.8.3"
  },
  "postinstall": "patch-package",
  "packageManager": "pnpm@10.13.1"
}
