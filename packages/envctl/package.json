{
  "name": "@cmux/envctl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "envctl": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "dev": "tsx watch src/index.ts",
    "test": "bash ./e2e/test.sh",
    "build:bin:linux": "bun build src/index.ts --compile --outfile dist/envctl-linux-x64",
    "build:bin:macos": "bun build src/index.ts --compile --outfile dist/envctl-macos-x64"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}