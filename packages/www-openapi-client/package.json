{
  "name": "@cmux/www-openapi-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/client/index.js",
  "types": "./dist/client/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/client/index.js",
      "types": "./dist/client/client/index.d.ts"
    },
    "./client.gen": {
      "import": "./dist/client/client.gen.js",
      "types": "./dist/client/client.gen.d.ts"
    },
    "./react-query": {
      "import": "./dist/client/@tanstack/react-query.gen.js",
      "types": "./dist/client/@tanstack/react-query.gen.d.ts"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.34.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5"
  }
}
