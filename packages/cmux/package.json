{
  "name": "cmux",
  "version": "0.1.1",
  "description": "Socket.IO and static file server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cmux": "./dist/cli.js"
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "tsc",
    "build:app": "echo 'Vite app should be built and copied to public/'",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "tsx src/test-env-leak.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "socket.io",
    "server",
    "static"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "socket.io": "^4.7.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}