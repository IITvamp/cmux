{
  "name": "cmux",
  "version": "0.1.1",
  "description": "Socket.IO and static file server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cmux": "./dist/cli.js"
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "tsc",
    "build:app": "echo 'Vite app should be built and copied to public/'",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "tsx scripts/test-env-leak.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prepare-publish": "npm run build && tsx scripts/prepare-publish.ts",
    "check-publish": "tsx scripts/check-upload-artifact.ts",
    "secure-publish": "npm run prepare-publish && npm run check-publish && echo 'Ready to publish! Run: cd upload-artifact && npm publish'"
  },
  "keywords": [
    "socket.io",
    "server",
    "static"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "socket.io": "^4.7.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.8.3",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}