{
  "name": "cmux",
  "version": "0.2.11",
  "description": "Socket.IO and static file server",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/lawrencecchen/cmux"
  },
  "bin": {
    "cmux": "./dist/cli.js"
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:app": "echo 'Vite app should be built and copied to public/'",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "tsx scripts/test-env-leak.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prepare-publish": "npm run build && tsx scripts/prepare-publish.ts",
    "check-publish": "tsx scripts/check-upload-artifact.ts",
    "secure-publish": "npm run prepare-publish && npm run check-publish && echo 'Ready to publish! Run: cd upload-artifact && npm publish'",
    "publish-cli": "tsx scripts/publish-npm.ts"
  },
  "keywords": [
    "socket.io",
    "server",
    "static"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cmux/server": "workspace:*",
    "commander": "^11.1.0",
    "convex": "1.26.0-alpha.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
