{
  "name": "@cmux/convex",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "./convex/_generated/server.js",
  "types": "./convex/_generated/server.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "test": "vitest run",
    "test:once": "vitest run",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text"
  },
  "exports": {
    ".": {
      "import": "./convex/_generated/server.js",
      "types": "./convex/_generated/server.d.ts"
    },
    "./api": {
      "import": "./convex/_generated/api.js",
      "types": "./convex/_generated/api.d.ts"
    },
    "./dataModel": {
      "import": "./convex/_generated/dataModel.d.ts",
      "types": "./convex/_generated/dataModel.d.ts"
    }
  },
  "dependencies": {
    "@convex-dev/migrations": "^0.2.9",
    "@stackframe/js": "^2.8.36",
    "@t3-oss/env-core": "^0.13.8",
    "convex": "1.26.0-alpha.6",
    "convex-helpers": "^0.1.104",
    "socket.io-client": "^4.8.1",
    "svix": "^1.75.0",
    "typescript": "^5.8.3",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^7.6.1",
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "vitest": "3.2.4"
  }
}